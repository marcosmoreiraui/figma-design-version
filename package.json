{
  "name": "design-version",
  "version": "2.0.1",
  "type": "module",
  "scripts": {
    "start": "run-s dev",
    "dev": "run-s watch",
    "dev:ui-only": "vite -c ./vite.config.ui.ts",
    "watch": "run-p watch:*",
    "watch:ui": "npm run build:ui -- --watch --mode=development ",
    "watch:plugin": "npm run build:plugin -- --watch --mode=development",
    "clean": "rimraf dist/*",
    "types": "run-s types:*",
    "types:src": "tsc",
    "types:node": "tsc -P tsconfig.node.json",
    "build": "run-s build:*",
    "prebuild": "run-s types clean",
    "build:ui": "vite build -c ./vite.config.ui.ts",
    "build:plugin": "vite build -c ./vite.config.plugin.ts"
  },
  "dependencies": {
    "@create-figma-plugin/utilities": "^3.2.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^3.0.2",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@figma/plugin-typings": "^1.90.0",
    "@types/node": "^18.19.31",
    "@types/postcss-url": "^10.0.4",
    "@types/react": "^18.2.75",
    "@types/react-dom": "^18.2.24",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.34.1",
    "npm": "^10.7.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.38",
    "postcss-url": "^10.1.3",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "sass": "^1.74.1",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vite-plugin-generate-file": "^0.1.1",
    "vite-plugin-react-rich-svg": "^1.1.1",
    "vite-plugin-singlefile": "^0.13.5"
  }
}
